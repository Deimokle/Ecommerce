<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>{% block title %}Welcome!{% endblock %}</title>
    {% block stylesheets %}{% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    <link rel="stylesheet" type="text/css" href="//maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css"/>
    <link rel="stylesheet" type="text/css" href="{{ asset('bundles/app/css/bootstrap.min.css') }}"/>
    <link rel="stylesheet" type="text/css" href="{{ asset('bundles/app/css/hover.css') }}" media="all" />
    <link rel="stylesheet" type="text/css" href="{{ asset('bundles/app/css/slider.css') }}" />
    <link rel="stylesheet" type="text/css" href="{{ asset('bundles/app/css/animate.css') }}" />
    <link rel="stylesheet" type="text/css" href="{{ asset('bundles/app/css/normalize.css') }}"/>
    <link rel="stylesheet" type="text/css" href="{{ asset('bundles/app/css/style-admin.css') }}"/>
</head>
<body>
<!-- Début menu -->
<nav class="navbar navbar-default">
    <div class="container-fluid">
        <!-- Brand and toggle get grouped for better mobile display -->
        <div class="navbar-header">
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand hvr-underline-from-left" href="#">CMS</a>
        </div>

        <!-- Collect the nav links, forms, and other content for toggling -->
        <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
            <ul class="nav navbar-nav navbar-right">
                <li><a class="hvr-underline-from-left" href="{{  path('ecommerce_admin') }}">Accueil</a></li>
                <li><a class="hvr-underline-from-left" href="{{  path('commande_index') }}">Commandes</a></li>
                <li><a class="hvr-underline-from-left" href="{{  path('produit_index') }}">Produits</a></li>
                <li><a class="hvr-underline-from-left" href="{{  path('abontnews_index') }}">Abonnés aux news</a></li>
                <li><a class="hvr-underline-from-left" href="{{  path('newsletter_index') }}">Newsletters</a></li>
                <li><a class="hvr-underline-from-left" href="{{  path('categorie_index') }}">Catégories des produits</a></li>
                <li><a class="hvr-underline-from-left" href="{{  path('tva_index') }}">TVA</a></li>
                <li><a class="hvr-underline-from-left" href="{{  path('lang_index') }}">Langues</a></li>
                
            </ul>
        </div><!-- /.navbar-collapse -->
    </div><!-- /.container-fluid -->
</nav>
<!-- Fin menu -->


{% block body %}{% endblock %}
{% block javascripts %}
    <!-- script -->

    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script type="text/javascript" src="{{ asset('bundles/app/js/jquery.parallax-1.1.3.js') }}"></script>
    <script src="{{ asset('bundles/app/js/jquery-1.9.1.min.js') }}"></script>
    <script src="{{ asset('bundles/app/js/docs.min.js') }}"></script>
    <script src="{{ asset('bundles/app/js/bootstrap.js') }}"></script>

    <!-- jssor slider scripts-->
    <!-- use jssor.slider.debug.js for debug -->
    <script type="text/javascript" src="{{ asset('bundles/app/js/jssor.slider.mini.js') }}"></script>

    <!--<script src="js/jquery-2.2.3.min.js"></script>-->
    <script src="{{ asset('bundles/app/js/typed.js') }}"></script>
    <!--<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.cycle/3.0.3/jquery.cycle.all.js"></script>-->

    <!-- Animation on scroll -->
    <script src="{{ asset('bundles/app/js/modernizr.custom.js') }}"></script>
    <script src="{{ asset('bundles/app/js/masonry.pkgd.min.js') }}"></script>
    <script src="{{ asset('bundles/app/js/imagesloaded.js') }}"></script>
    <script src="{{ asset('bundles/app/js/classie.js') }}"></script>
    <script src="{{ asset('bundles/app/js/AnimOnScroll.js') }}"></script>
{% endblock %}


<footer style="height: 100px;"></footer>
</body>
</html>