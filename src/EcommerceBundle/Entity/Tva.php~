<?php

namespace EcommerceBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Tva
 */
class Tva
{
    /**
     * @var int
     */
    private $id;

    /**
     * @var bool
     */
    private $tvaEtat;

    /**
     * @var string
     */
    private $tvaLib;

    /**
     * @var string
     */
    private $tvaTaux;

    /**
     * @var \DateTime
     */
    private $tvaDteDesact;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set tvaEtat
     *
     * @param boolean $tvaEtat
     * @return Tva
     */
    public function setTvaEtat($tvaEtat)
    {
        $this->tvaEtat = $tvaEtat;

        return $this;
    }

    /**
     * Get tvaEtat
     *
     * @return boolean 
     */
    public function getTvaEtat()
    {
        return $this->tvaEtat;
    }

    /**
     * Set tvaLib
     *
     * @param string $tvaLib
     * @return Tva
     */
    public function setTvaLib($tvaLib)
    {
        $this->tvaLib = $tvaLib;

        return $this;
    }

    /**
     * Get tvaLib
     *
     * @return string 
     */
    public function getTvaLib()
    {
        return $this->tvaLib;
    }

    /**
     * Set tvaTaux
     *
     * @param string $tvaTaux
     * @return Tva
     */
    public function setTvaTaux($tvaTaux)
    {
        $this->tvaTaux = $tvaTaux;

        return $this;
    }

    /**
     * Get tvaTaux
     *
     * @return string 
     */
    public function getTvaTaux()
    {
        return $this->tvaTaux;
    }

    /**
     * Set tvaDteDesact
     *
     * @param \DateTime $tvaDteDesact
     * @return Tva
     */
    public function setTvaDteDesact($tvaDteDesact)
    {
        $this->tvaDteDesact = $tvaDteDesact;

        return $this;
    }

    /**
     * Get tvaDteDesact
     *
     * @return \DateTime 
     */
    public function getTvaDteDesact()
    {
        return $this->tvaDteDesact;
    }
}
